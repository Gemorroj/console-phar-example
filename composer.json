{
    "name": "gemorroj/console-phar-example",
    "description": "Example symfony console application and packed with box2 ",
    "keywords": ["console", "phar"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "symfony/console": "^3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.3",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "ConsolePharExample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConsolePharExample\\Tests\\": "tests/"
        }
    }
}
